    <script>


        function unlistbanner(bannerid) {
            Swal.fire({
                title: 'Are you sure?',
                text: "Do you want to unlist this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, unlist it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    console.log(bannerid)
                    $.ajax({
                        url: '/admin/unlist-banner?bannerid=' + bannerid,
                        method: 'get',
                        success: (responce) => {
                            if (responce.status === true) {
                                console.log(responce.status)
                                Swal.fire(
                                    'unlisted!',
                                    'Banner has been unlisted.',
                                    'success'
                                ).then(()=>{
                                    window.location.href = '/admin/load-Banners'
                            })
                            }
                        }
                    })
                }
            })
        }

        
        function listbanner(bannerid) {
            Swal.fire({
                title: 'Are you sure?',
                text: "Do you want to list this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes!'
            }).then((result) => {
                if (result.isConfirmed) {
                    console.log(bannerid)
                    $.ajax({
                        url: '/admin/list-banner?bannerid=' + bannerid,
                        method: 'get',
                        success: (responce) => {
                            if (responce.status === true) {
                                console.log(responce.status)
                                Swal.fire(
                                    'Listed',
                                    'Banner has been listed.',
                                    'success'
                                ).then(()=>{
                                    window.location.href = '/admin/load-Banners'
                            })
                            }
                        }
                    })
                }
            })
        }



