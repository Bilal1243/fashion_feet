        <!-- content section starts -->
            <section class="content-main">
                <div class="user-table-container container">
                    <section class="content-main">
                      <h2 class="mb-4">User Listing</h2>
                      <table class="table table-bordered table-striped user-table">
                        <thead>
                          <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Mobile no.</th>
                            <th>Block/unblock</th>
                          </tr>
                        </thead>
                        <tbody>
                            <% if(user.length > 0){ %>
                                <% user.reverse()%>
                                <% for(let i=0; i<user.length; i++){ %>
                                    <tr>
                                        <td><%= i+1 %></td>
                                        <td><%= user[i].name %></td>
                                        <td><%= user[i].email %></td>
                                        <td><%= user[i].mobile %></td>
                                        <td><% if (user[i].is_block == 1) { %>
                                            <!-- Display Unblock button if the user is blocked  -->
                                            <a href="/admin/unblockuser?id=<%= user[i]._id %>" class="btn btn-success">Unblock</a>
                                          <% } else { %> 
                                            <!-- Display Block button if the user is not blocked -->
                                            <a href="/admin/blockuser?id=<%= user[i]._id %>" class="btn btn-danger">Block</a>
                                          <% } %>
                                        </td>
                                    </tr>
                                <% } %>
                            <% } else { %>
                                <tr>
                                    <td colspan="4">Users Not found</td>
                                </tr>
                            <% } %>
                        </tbody>
                      </table>
                    </section>
                  </div>
            </section>
