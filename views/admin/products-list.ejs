                <a href="javascript:history.back()"><i class="material-icons md-arrow_back"></i> Go back </a>
            </div>
            <% products.reverse()%>
                <% for (let i=0; i < products.length; i++) { const product=products[i]; %>
                    <article class="itemlist">
                        <div class="row align-items-center">
                            <div class="col col-check flex-grow-0">
                                <div class="form-check">
                                    <p>
                                        <%= i+1 %>
                                    </p>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
                                <a class="itemside" href="#">
                                    <div class="left">
                                        <img src="/admin-assets/uploads/<%=product.image[0]%>"
                                            class="img-sm img-thumbnail" alt="Item">
                                    </div>
                                    <div class="info">
                                        <h6 class="mb-0">
                                            <%= product.product_name %>
                                        </h6>
                                    </div>
                                </a>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-price"> <span>
                                    <%= product.offer_price %>
                                </span> </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-status">
                                <span class="badge rounded-pill alert-success">
                                    <%= product.brand_name %>
                                </span>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                                <% if (product.is_listed) { %>
                                    <!-- Display Unblock button if the user is blocked  -->
                                    <button class="btn btn-sm font-sm rounded btn-brand mb-2"
                                        onclick="unlist('<%=product._id%>')">Unlist</button>
                                    <!-- <a href="/admin/unlist-product?id=<%= product._id %>" class="btn btn-sm font-sm rounded btn-brand mb-2">Unlist</a> -->
                                    <% } else { %>
                                        <!-- Display Block button if the user is not blocked -->
                                        <button class="btn btn-sm font-sm rounded btn-brand mb-2"
                                            onclick="listing('<%= product._id %>')">List</button>
                                        <!-- <a href="/admin/list-product?id=<%= product._id %>" class="btn btn-sm font-sm rounded btn-brand mb-2">List</a> -->
                                        <% } %>
                                            <a href="/admin/edit-product?id=<%= product._id %>"
                                                class="btn btn-sm font-sm rounded btn-brand mb-2">
                                                <i class="material-icons md-edit"></i> Edit
                                            </a>
                            </div>
                        </div> <!-- row .// -->
                    </article>
                    <% } %>

