    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/toastify-js"></script>
    <script>
        function listing(id) {
            console.log("etner")
            $.ajax({
                url: '/admin/list-product?id=' + id,
                method: 'PUT',
                success: (response) => {
                    if (response.status === true) {
                        Toastify({
                            text: "Product Listed Succefully",
                            className: "info",
                            style: {
                                background: "linear-gradient(to right, #00ff80, #03994e)",
                            }
                        }).showToast();
                        setTimeout(() => {
                            location.href = '/admin/product-list'
                        }, 1000);
                    }
                    else{
                        Toastify({
                            text: "category unlisted or category not found",
                            className: "info",
                            style: {
                                background: "linear-gradient(to right, #b57679, #f53b44)",
                            }
                        }).showToast();
                    }
                }
            })
        }
        
        function unlist(id) {
            $.ajax({
                url: '/admin/unlist-product?id=' + id,
                method: 'PUT',
                success: (response) => {
                    if (response.status === true) {
                        Toastify({
                            text: "Product Unlisted Succefully",
                            className: "info",
                            style: {
                                background: "linear-gradient(to right, #00ff80, #03994e)",
                            }
                        }).showToast();
                        setTimeout(() => {
                            location.href = '/admin/product-list'
                        }, 1000);
                    }
                    else{
                        Toastify({
                            text: "error",
                            className: "info",
                            style: {
                                background: "linear-gradient(to right, #00b09b, #96c93d)",
                            }
                        }).showToast();
                    }
                }
            })
        }
    </script>

