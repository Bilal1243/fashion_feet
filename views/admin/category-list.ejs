    <script>
        function UnlistCategory(id) {
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, delete it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    $.ajax({
                        url: '/admin/unlist-category?id=' + id,
                        method: 'get',
                        success: (response) => {
                            if (response.status === true) {
                                Swal.fire(
                                    'Unlisted!',
                                    'Your file has been unlisted.',
                                    'success'
                                ).then(()=> {
                                    location.href = '/admin/category-list'
                                })
                            }
                        }
                    })
                }
            })
        }
        function listCategory(id) {
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, List it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    $.ajax({
                        url: '/admin/list-category?id=' + id,
                        method: 'get',
                        success: (response) => {
                            if (response.status === true) {
                                Swal.fire(
                                    'Unlisted!',
                                    'Your file has been unlisted.',
                                    'success'
                                ).then(()=> {
                                    location.href = '/admin/category-list'
                                })
                            }
                        }
                    })
                }
            })
        }
    </script>

