                                        alt="Logo">
                                </div>
                                <figcaption class="card-body text-center">
                                    <h6 class="card-title m-0">
                                        <%= category.category_name %>
                                    </h6>
                                    <p><%=category.offer%>%</p>
                                    <div class="d-flex justify-content-center align-items-center">
                                        <a href="/admin/edit-category?id=<%= category._id %>"
                                            class="btn btn-sm font-sm rounded btn-brand me-2">
                                            <i class="material-icons md-edit"></i> Edit
                                        </a>
                                       <%if(category.is_listed === true){%>
                                        <button class="btn btn-sm font-sm btn-light rounded"
                                        onclick="UnlistCategory('<%= category._id %>')"><i class="fa-solid fa-eye-slash"></i>Unlist
                                        </button>
                                       <%} else {%>
                                        <button class="btn btn-sm font-sm btn-light rounded"
                                        onclick="listCategory('<%= category._id %>')"><i class="fa-solid fa-eye"></i>List
                                        </button>
                                       <%}%>
                                    </div>
                                </figcaption>
                            </figure>
                        </div>
                        <% } %>
