        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupons</h2>
                    <p>Available Coupons.</p>
                </div>
            </div>
            <%if(coupons.length < 1) {%>
                <p class="text-muted">no coupons</p>
            <%} else {%>
                <div class="card mb-4">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>#ID</th>
                                        <th scope="col">Code</th>
                                        <th scope="col">Offer</th>
                                        <th scope="col">Start</th>
                                        <th scope="col">Expire</th>
                                        <th scope="col">Minimum</th>
                                        <th scope="col">maximum</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% coupons.forEach(coupon => {%>
                                        <tr>
                                            <td><%=coupon._id%></td>
                                            <td><b><%=coupon.coupon%></b></td>
                                            <td><%=coupon.offer%></td>
                                            <td><span class="badge rounded-pill alert-warning"><%=coupon.start.toLocaleDateString()%></span></td>
                                            <td><span class="badge rounded-pill alert-warning"><%=coupon.validity.toLocaleDateString()%></span></td>
                                            <td><%=coupon.min_price%></td>
                                            <td><%=coupon.max_price%></td>
                                        </tr>
                                    <%})%>
                                </tbody>
                            </table>
                        </div> <!-- table-responsive //end -->
                    </div> <!-- card-body end// -->
                </div>
            <%}%>
            <!-- card end// -->
        </section> <!-- content-main end// -->
